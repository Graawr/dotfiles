!-------------------------------------------------------------------------------
!!! XFT SETTINGS !!!
!-------------------------------------------------------------------------------
     
Xft.dpi:                              96
Xft.antialias:                        true
Xft.rgba:                             rgb
Xft.hinting:                          true
Xft.hintstyle:                        hintslight

     
!-------------------------------------------------------------------------------
!!! URXVT SETTINGS !!!
!-------------------------------------------------------------------------------

urxvt*depth:                          33
URxvt*geometry:                       80x25
URxvt*internalBorder:                 8
URxvt*borderLess:                     false
URxvt*scrollBar:                      false
URxvt*cursorBlink:                    true
URxvt*cursorUnderline:                true
URxvt*scrollColor:                    #181513
URxvt.underlineURLs:                  true

URxvt.url-select.underline:           true
URxvt.url-select.launcher:            /usr/bin/iceweasel
URxvt*perl-ext-common:                default,clipboard,url-select,keyboard-select,vtwheel
URxvt.keysym.C-c:                     perl:clipboard:copy
URxvt.keysym.C-p:                     perl:clipboard:paste
URxvt.keysym.M-C-v:                   perl:clipboard:paste_escaped
URxvt.keysym.M-Escape:                perl:keyboard-select:activate
URxvt.keysym.M-u:                     perl:url-select:select_next
 

!-------------------------------------------------------------------------------
!!! THEME !!!
!-------------------------------------------------------------------------------
*font:                                -*-termsyn-medium-*-*-*-11-*-*-*-*-*-*-*
*boldFont:                            -*-termsyn-bold-*-*-*-11-*-*-*-*-*-*-*
*italicFont:                          -*-termsyn-medium-*-*-*-11-*-*-*-*-*-*-*
*boldItalicFont:                      -*-termsyn-bold-*-*-*-11-*-*-*-*-*-*-*

*cursorColor:                         #917154
*colorIT:                             #E3AB8D
*colorBD:                             #F0E5DF
*colorUL:                             #cd546c
*VT100.colorBDMode:                   false
*VT100.colorBD:                       blue
*VT100.colorULMode:                   true
*VT100.colorUL:                       blue
*VT100.veryBoldColors:                6


